    "@supabase/supabase-js": "^2.89.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/multer": "^2.0.0",
    "@types/pg": "^8.11.14",
    "@types/uuid": "^10.0.0",
    "compression": "^1.8.0",
    "connect-pg-simple": "^10.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.38.4",
    "drizzle-seed": "^0.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.1",
    "file-type": "^16.5.4",
    "helmet": "^8.1.0",
    "multer": "^2.0.2",
    "nanoid": "^5.1.5",
    "node-fetch": "^3.3.2",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.15.6",
    "postgres": "^3.4.7",
    "qrcode": "^1.5.4",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "ws": "^8.18.0",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@types/connect-pg-simple": "^7.0.3",
